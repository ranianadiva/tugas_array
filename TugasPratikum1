public class TugasPraktikum1 {

    public static void main(String[] args) {
        int[] bilangan;
        bilangan = new int[5];
        bilangan[0] = 1;
        bilangan[1] = 2;
        bilangan[2] = 3;
        bilangan[3] = 4;
        bilangan[4] = 5;
        for (int i = 0; i < bilangan.length; i++) {

            System.out.println("bilangannya adalah " + i + "=" + bilangan[4]);
        }
    }
}
